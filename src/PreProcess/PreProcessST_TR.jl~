function preprocessST_TR(H, g, params::TparamsST, n1, n2)
    τ = params.τ

    return  PDataST_TR(H,g,τ,true)
end

function Decrease(X :: PDataST, α:: Float64, TR:: TrustRegion)
    return Decrease(α, TR)
end

function Increase(X :: PDataST, α:: Float64, TR:: TrustRegion)
    return Increase(α, TR)
end

