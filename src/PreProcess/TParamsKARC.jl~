type TparamsKARC <: Tparams  # specific parameters for this Krylov variant
    τ :: Float64
    shifts :: Array{Float64,1}
    nshifts :: Int
    function TparamsKARC()
        τ = 0.75
        shifts = 10.0.^(collect(-15.0:1.0:15.0))
        nshifts = size(shifts, 1)
        return new(τ,shifts,nshifts)
    end
end
